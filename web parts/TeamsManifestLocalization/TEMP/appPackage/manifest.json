{
	"$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.16/MicrosoftTeams.schema.json",
	"version": "1.0.2",
	"manifestVersion": "1.16",
	"id": "${{TEAMS_APP_ID}}",
	"packageName": "com.sample.localization",
	"name": {
		"short": "Localization sample",
		"full": "Localization sample full name"
	},
	"developer": {
		"name": "guidozam",
		"mpnId": "",
		"websiteUrl": "https://iamguidozam.blog",
		"privacyUrl": "https://iamguidozam.blog",
		"termsOfUseUrl": "https://iamguidozam.blog"
	},
	"description": {
		"short": "Localization sample short description",
		"full": "Localization sample long description"
	},
	"icons": {
		"outline": "outline.png",
		"color": "color.png"
	},
	"accentColor": "#07525B",
	"staticTabs": [
		{
			"entityId": "3a0735b6-db97-4e9c-9950-e948808e191c",
			"name": "Hello World",
			"contentUrl": "${{TAB_ENDPOINT}}/tab",
			"scopes": [
				"personal"
			],
			"context": [
				"personalTab"
			]
		},
		{
			"entityId": "about",
			"scopes": [
				"personal"
			]
		}
	],
	"validDomains": [
		"${{TAB_DOMAIN}}"
	],
	"localizationInfo": {
		"defaultLanguageTag": "en-us",
		"additionalLanguages": [
			{
				"languageTag": "it",
				"file": "it.json"
			}
		]
	}
}