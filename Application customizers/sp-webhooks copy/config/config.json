{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "notification-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/notification/NotificationApplicationCustomizer.js",
          "manifest": "./src/extensions/notification/NotificationApplicationCustomizer.manifest.json"
        }
      ]
    },
    "handle-notification-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/handleNotification/HandleNotificationApplicationCustomizer.js",
          "manifest": "./src/extensions/handleNotification/HandleNotificationApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "NotificationApplicationCustomizerStrings": "lib/extensions/notification/loc/{locale}.js",
    "HandleNotificationApplicationCustomizerStrings": "lib/extensions/handleNotification/loc/{locale}.js"
  }
}
